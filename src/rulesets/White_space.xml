<?xml version="1.0" encoding="UTF-8"?>
<ruleset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ruleset.xsd">
  <title>White space</title>
  <rule>
    <report>tab character</report>
    <regex>
      <find>\t</find>
      <replace>  </replace>
    </regex>
  </rule>
</ruleset>

